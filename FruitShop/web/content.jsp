<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>JSP Page</title>
    </head>
    <body>
        <!-- Product Listings Section -->
        <div class="container">
            <div class="row">
                <!-- Product 1 -->
                <div class="col-md-4">
                    <div class="card">
                        <img src="images/RedDeliciousApples.jpg" class="card-img-top" alt="Apple">
                        <div class="card-body">
                            <h5 class="card-title">Red Delicious Apples</h5>
                            <p class="card-text">Sweet and crispy apples.</p>
                            <p class="card-text">Price: $0.75/kg</p>
                            <a href="#" class="btn btn-primary">Add to Cart</a>
                        </div>
                    </div>
                </div>

                <!-- Product 2 -->
                <div class="col-md-4">
                    <div class="card">
                        <img src="images/NavelOranges.jpg" class="card-img-top" alt="Apple">
                        <div class="card-body">
                            <h5 class="card-title">Navel Oranges</h5>
                            <p class="card-text">Juicy and seedless oranges</p>
                            <p class="card-text">Price: $35/kg</p>
                            <a href="#" class="btn btn-primary">Add to Cart</a>
                        </div>
                    </div>
                </div>

                <!-- Product 3 -->
                <div class="col-md-4">
                    <div class="card">
                        <img src="images/Pineapples.jpg" class="card-img-top" alt="Apple">
                        <div class="card-body">
                            <h5 class="card-title">Pineapples</h5>
                            <p class="card-text">Exotic and tropical pineapples</p>
                            <p class="card-text">Price: $42/kg</p>
                            <a href="#" class="btn btn-primary">Add to Cart</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>
